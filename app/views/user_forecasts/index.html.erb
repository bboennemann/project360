<p id="notice"><%= notice %></p>

<h1>Listing User Forecasts</h1>

<table>
  <thead>
    <tr>
      <th>Timeentry</th>
      <th>Published</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_forecasts.each do |user_forecast| %>
      <tr>
        <td><%= user_forecast.timeentry %></td>
        <td><%= user_forecast.published %></td>
        <td><%= link_to 'Show', user_forecast %></td>
        <td><%= link_to 'Edit', edit_user_forecast_path(user_forecast) %></td>
        <td><%= link_to 'Destroy', user_forecast, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User forecast', new_user_forecast_path %>
